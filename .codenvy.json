        "attributes": {
          "maven.artifactId":[
            "ballshop"
          ],
          "maven.parent.version":[
            ""
          ],
          "maven.parent.groupId":[
            ""
          ],
          "maven.version":[
            "1.0.0-SNAPSHOT"
          ],
